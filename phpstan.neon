parameters:
    level: 1
    autoload_directories:
        - Api
        - Block
        - Controller
        - Helper
        - Model
        - ViewModel
    excludes_analyse:
        - 'vendor'
    ignoreErrors:
        - '#invalid typehint#'
        - '#unknown class#'
        - '#Pager#'
    reportUnmatchedIgnoredErrors: false