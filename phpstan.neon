parameters:
    level: 1
    autoload_directories:
        - Api
        - Block
        - Controller
        - Helper
        - Model
        - ViewModel
    excludes_analyse:
        - 'vendor'
        - 'Block\Adminhtml\System'
    ignoreErrors:
        - '#invalid typehint#'
        - '#unknown class#'
    reportUnmatchedIgnoredErrors: false